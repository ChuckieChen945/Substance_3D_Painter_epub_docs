        prioritize="filtersSidebarCtrl.prioritizedLanguage"
        menu-items="filtersSidebarCtrl.getLanguages()"
        selected-value="filtersSidebarCtrl.selectedLanguage"
        on-selected-item-change="filtersSidebarCtrl.handleSelectedLanguageModeChange()"
        trigger-button-data-id="languages-and-writing-systems-button">
        Select an option...
      </spectrum-dropdown>
    </div>
  </div>

  <!-- Library filters -->
  <div class="adobe-fonts-filters-sidebar__section spectrum-Accordion-item"
       ng-class="{ 'is-open': filtersSidebarCtrl.isSectionOpen('library') }"
       ng-show="filtersSidebarCtrl.isLibrarySectionVisible()">
    <h2 class="spectrum-Accordion-itemHeader spectrum-Heading--subtitle1 adobe-fonts-filters-sidebar__section-header"
        ng-click="filtersSidebarCtrl.handleSectionToggleClick('library')" data-id="library-filter">
      Library

      <svg class="spectrum-Icon spectrum-UIIcon-ChevronRightMedium spectrum-Accordion-itemIndicator adobe-fonts-filters-sidebar__section-arrow"
           focusable="false"
           aria-hidden="true">
        <use xlink:href="#spectrum-css-icon-ChevronRightMedium" />
      </svg>
    </h2>

    <!-- Paid fonts filter -->
    <div class="spectrum-Accordion-itemContent adobe-fonts-filters-sidebar__section-content">
      <label class="spectrum-Checkbox">
        <input type="checkbox"
               class="spectrum-Checkbox-input"
               ng-checked="filtersSidebarCtrl.isShowPaidFontsSelected()"
               ng-click="filtersSidebarCtrl.handleShowPaidFontsChange()">

        <span class="spectrum-Checkbox-box">
          <svg class="spectrum-Icon spectrum-UIIcon-CheckmarkSmall spectrum-Checkbox-checkmark"
               focusable="false"
               aria-hidden="true">
            <use xlink:href="#spectrum-css-icon-CheckmarkSmall" />
          </svg>
        </span>

        <span class="adobe-fonts-filters-sidebar__section-checkbox-label">
          Show fonts available with Creative Cloud.
        </span>
      </label>
    </div>
  </div>

  <!-- Tags filters -->
  <div class="adobe-fonts-filters-sidebar__section spectrum-Accordion-item"
       ng-class="{ 'is-open': filtersSidebarCtrl.isSectionOpen('tags') }"
       ng-show="filtersSidebarCtrl.isTagsSectionVisible()">
    <h2 class="spectrum-Accordion-itemHeader spectrum-Heading--subtitle1 adobe-fonts-filters-sidebar__section-header"
        ng-click="filtersSidebarCtrl.handleSectionToggleClick('tags')">
      TAGS
      <svg class="spectrum-Icon spectrum-UIIcon-ChevronRightMedium spectrum-Accordion-itemIndicator adobe-fonts-filters-sidebar__section-arrow"
           focusable="false"
           aria-hidden="true">
        <use xlink:href="#spectrum-css-icon-ChevronRightMedium" />
      </svg>
    </h2>

    <div class="spectrum-Accordion-itemContent adobe-fonts-filters-sidebar__section-content
                adobe-fonts-filters-sidebar__tags-filters-container">
      <ul class="list--inline adobe-fonts-filters-sidebar__tags-filters-section spectrum-Tags">
        <li ng-repeat="tag in filtersSidebarCtrl.getTags() | limitTo:filtersSidebarCtrl.getDisplayTagsCount()"
            ng-class="{ selected: filtersSidebarCtrl.isTagSelected(tag.key) }">
          <button class="adobe-fonts-filters-sidebar__tags-filter-button spectrum-Tags-item adobe-fonts-filters-sidebar__tags-filter-button-display-variation"
                  data-id="tag-{{tag.key}}"
                  ng-if="!filtersSidebarCtrl.shouldShowDisplayFont(tag)"
                  ng-click="filtersSidebarCtrl.handleTagClick(tag.key)">
            <span class="spectrum-Tags-item-label">
              {{ tag.label }}
            </span>
          </button>
          <button class="adobe-fonts-filters-sidebar__tags-filter-button spectrum-Tags-item adobe-fonts-filters-sidebar__tags-filter-button-display-variation"
                  data-id="tag-{{tag.key}}"
                  display-in-font="filtersSidebarCtrl.getFontDisplayData(tag)"
                  ng-if="filtersSidebarCtrl.shouldShowDisplayFont(tag)"
                  ng-click="filtersSidebarCtrl.handleTagClick(tag.key)">
            <span class="spectrum-Tags-item-label">
              {{ tag.label }}
            </span>
          </button>
        </li>
      </ul>
      <span class="adobe-fonts-filters-sidebar__tags-filter-expand-button"
            ng-if="filtersSidebarCtrl.shouldDisplayShowMore()"
            ng-click="filtersSidebarCtrl.toggleShowMore()">
        View {{filtersSidebarCtrl.getDisplayMoreCount()}} more
      </span>
      <span class="adobe-fonts-filters-sidebar__tags-filter-expand-button"
            ng-if="filtersSidebarCtrl.shouldDisplayShowLess()"
            ng-click="filtersSidebarCtrl.toggleShowMore()">
            Show less
      </span>
    </div>
  </div>

  <!-- Filters based on data from the backend -->
  <div class="adobe-fonts-filters-sidebar__section spectrum-Accordion-item"
       ng-repeat="filterSection in filtersSidebarCtrl.getCurrentFilters()"
       ng-class="{ 'is-open': filtersSidebarCtrl.isSectionOpen(filterSection.name) }">
    <h2 class="spectrum-Accordion-itemHeader spectrum-Heading--subtitle1 adobe-fonts-filters-sidebar__section-header"
        ng-click="filtersSidebarCtrl.handleSectionToggleClick(filterSection.name)">
      {{ filterSection.label }}

      <svg class="spectrum-Icon spectrum-UIIcon-ChevronRightMedium spectrum-Accordion-itemIndicator adobe-fonts-filters-sidebar__section-arrow"
           focusable="false"
           aria-hidden="true">
        <use xlink:href="#spectrum-css-icon-ChevronRightMedium" />
      </svg>
    </h2>

    <div class="spectrum-Accordion-itemContent adobe-fonts-filters-sidebar__section-content">
      <div class="adobe-fonts-filters-sidebar__styles-slider-section" data-id="number-of-styles-slider"
           ng-if="filtersSidebarCtrl.shouldDisplayStylesSlider(filterSection)">
        <spectrum-slider
          allow-overlap="true"
          end-value="filtersSidebarCtrl.getMaxStyles()"
          label="Number of fonts in family"
          max="26"
          min="1"
          on-change="filtersSidebarCtrl.handleStylesSliderChange($data)"
          start-value="filtersSidebarCtrl.getMinStyles()"
          step="1"
          value-label="filtersSidebarCtrl.getStylesSliderLabel($data)"
          variant="range"></spectrum-slider>
      </div>

      <ul class="list--inline adobe-fonts-filters-sidebar__language-mode-filters"
          ng-if="!filtersSidebarCtrl.isGrouped(filterSection)">
        <li class="adobe-fonts-filters-sidebar__language-mode-filter-item"
            ng-repeat="filter in filterSection.values"
            ng-class="{ selected: filtersSidebarCtrl.isFilterSelected(filter.key) }">
          <button ng-attr-id="{{ filter.slug }}"
                  class="adobe-fonts-filters-sidebar__filter-button"
                  ng-click="filtersSidebarCtrl.handleFilterClick(filter.key)">
            <svg class="adobe-fonts-filters-sidebar__icon--medium">
              <use ng-attr-xlink:href="{{ filtersSidebarCtrl.getIconUrlForFilter(filter) }}"
                   xlink:href=""
                   class="adobe-fonts-filters-sidebar__icon"></use>
            </svg>
          </button>
          <label for="{{ filter.slug }}">{{ filter.label }}</label>
        </li>
      </ul>

      <ul class="list--no-decoration adobe-fonts-filters-sidebar__filter-groups"
          ng-if="filtersSidebarCtrl.isGrouped(filterSection)">
        <li class="adobe-fonts-filters-sidebar__filter-group adobe-fonts-filters-sidebar__filter-group--{{filter.key}}"
            ng-repeat="filter in filterSection.filters">
          <ul class="list--no-decoration">
            <li class="adobe-fonts-filters-sidebar__filter-option"
                ng-class="{ selected: filtersSidebarCtrl.isFilterSelected(value.key) }"
                ng-repeat="value in filter.values">
              <button ng-attr-id="{{ value.slug }}"
                      ng-click="filtersSidebarCtrl.handleFilterClick(value.key)">
                <svg class="adobe-fonts-filters-sidebar__icon--medium">
                  <use ng-attr-xlink:href="{{ filtersSidebarCtrl.getIconUrlForFilter(value) }}" xlink:href=""></use>
                </svg>
              </button>
            </li>
          </ul>

          <label class="spectrum-Radio-label">
            {{ filter.label }}
          </label>
        </li>
      </ul>
    </div>
  </div>
</div>
